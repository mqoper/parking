<p id="notice"><%= notice %></p>

<h1>Parking spot history</h1>

<table>
  <thead>
  <tr>
    <th>Spot name</th>
    <th>User</th>
  </tr>
  </thead>

  <tbody>
  <% @History.each do |history| %>
    <tr>
      <td><%= history.user.username if history.user %></td>
      <td><%= history.spot.name %></td>
    </tr>
  <% end %>
  </tbody>
</table>