<p id="notice"><%= notice %></p>

<h1>Parking spot history</h1>

<table>
  <thead>
  <tr>
    <th>Spot name</th>
    <th>User</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @History.each do |history| %>
    <tr>
      <td><%= history.name %></td>
      <td><%= history.reserved %></td>
<!--      <td><%#= link_to 'Show', spot %></td>-->
<!--      <td><%#= link_to 'Edit', edit_spot_path(spot) %></td>-->
      <td><%= link_to 'Destroy', history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
  </tbody>
</table>

